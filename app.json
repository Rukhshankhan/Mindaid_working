{
  "expo": {
    "name": "MindAid",
    "slug": "mindAid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/do.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/do.png",
      "resizeMode": "contain",
      "backgroundColor": "#6B48FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyA3FzKFHiA7bUcmOaubinG6wqCZt8Dw7Yk"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is needed to send to emergency contacts in case of emergency",
        "NSLocationAlwaysUsageDescription": "Your location is needed to send to emergency contacts in case of emergency",
        "NSAppleMusicUsageDescription": "Allow access to play meditation music from your library",
        "NSContactsUsageDescription": "Access to contacts is needed to send emergency messages",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Your location is needed to send to emergency contacts in case of emergency"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/do.png",
        "backgroundColor": "#6B48FF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_CONTACTS",
        "CALL_PHONE",
        "SEND_SMS",
        "READ_PHONE_STATE",
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA3FzKFHiA7bUcmOaubinG6wqCZt8Dw7Yk"
        }
      },
      "package": "com.k214663.mindAid"
    },
    "scheme": "your-scheme",
    "web": {
      "config": {
        "spotify": {
          "clientId": "your_client_id"
        }
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos for your profile and journal."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "19231e10-c5e6-4adf-8ab3-de4c302b78df"
      }
    },
    "owner": "k214663"
  }
}
