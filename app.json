{
  "expo": {
    "name": "MindBloom",
    "slug": "mindbloom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6B48FF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyA3FzKFHiA7bUcmOaubinG6wqCZt8Dw7Yk"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to find meditation centers near you.",
        "NSLocationAlwaysUsageDescription": "This app needs access to location to find meditation centers near you.",
        "NSAppleMusicUsageDescription": "Allow access to play meditation music from your library",
        "NSContactsUsageDescription": "Allow access to contacts to add emergency contacts"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6B48FF"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "READ_CONTACTS", "CALL_PHONE"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA3FzKFHiA7bUcmOaubinG6wqCZt8Dw7Yk"
        }
      }
    },
    "scheme": "your-scheme",
    "web": {
      "config": {
        "spotify": {
          "clientId": "your_client_id"
        }
      }
    }
  }
}
